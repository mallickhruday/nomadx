{
  "name": "@nomadx/table",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/nomadx-table.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/nomadx-table.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@nomadx/utils": "^0.1.0",
    "@stencil/sass": "0.0.5",
    "clipboard": "^2.0.1",
    "json5": "^1.0.1",
    "marked": "^0.4.0",
    "marktable": "^0.1.2",
    "natural-compare": "^1.4.0",
    "papaparse": "^4.4.0"
  },
  "devDependencies": {
    "@stencil/core": "^0.9.1",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natemoo-re/nomadx-table.git"
  },
  "author": "Nate Moore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natemoo-re/nomadx-table"
  },
  "homepage": "https://github.com/natemoo-re/nomadx-table",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
